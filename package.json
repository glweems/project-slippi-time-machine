{
  "author": "glweems",
  "dependencies": {
    "@slippi/slippi-js": "^6.1.3",
    "chokidar": "^3.5.2",
    "express": "^4.17.2",
    "inotifywait": "^1.8.3",
    "lowdash": "^1.2.0",
    "mkdirp": "^1.0.4",
    "remove": "^0.1.5",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  },
  "description": "script to persist slippi game data",
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^5.8.0",
    "@typescript-eslint/parser": "^5.8.0",
    "eslint": "^8.5.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.3",
    "nodemon": "^2.0.15"
  },
  "keywords": [
    "slippi",
    "melee",
    "project-slippi"
  ],
  "license": "GPL-3.0",
  "main": "index.js",
