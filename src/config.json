{
  "name": "project-slippi-time-machine",
  "description": "script to persist slippi game data",
  "license": "GPL-3.0",
  "author": "Garrett Weems <gwgraphicdesign@gmail.com> (https://glweems.com)",
  "version": "1.0.0",
  "types": "lib/types/*.d.ts",
  "keywords": [
    "slippi",
    "melee",
    "project-slippi"
  ],
  "scripts": {
    "lint": "eslint \"src/**/*.ts\"",
    "lint:fix": "yarn run lint --fix",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "nodemon -e ts -w ./src -x npm run-script build",
    "watch:serve": "ts-node src/index.ts",
    "db:seed": "prisma db seed",
    "clean": "rm -rf lib",
    "build:esm": "tsc -p tsconfig.json",
    "build:cjs": "tsc -p tsconfig-cjs.json",
    "build": "cp package.json src/config.json && npm run-script build:esm && npm run-script build:cjs"
  },
  "dependencies": {
    "@prisma/client": "^3.7.0",
    "@slippi/slippi-js": "^6.1.3",
    "chalk": "^5.0.0",
    "cli-progress": "^3.10.0",
    "commander": "^8.3.0",
    "cp-file": "^9.1.0",
    "delete-empty": "^3.0.0",
    "dotenv": "^10.0.0",
    "filename-reserved-regex": "^3.0.0",
    "filenamify": "^5.1.0",
    "fs-extra": "^10.0.0",
    "glob": "^7.2.0",
    "lodash": "^4.17.21",
    "make-dir": "^3.1.0",
    "moment": "^2.29.1",
    "prompts": "^2.4.2",
    "remove": "^0.1.5",
    "revalidator": "^0.3.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  },
  "devDependencies": {
    "@types/cli-progress": "^3.9.2",
    "@types/delete-empty": "^3.0.2",
    "@types/fs-extra": "^9.0.13",
    "@types/glob": "^7.2.0",
    "@types/lodash": "^4.14.178",
    "@types/node": "^17.0.8",
    "@types/prompt": "^1.1.2",
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "@typescript-eslint/parser": "^5.9.0",
    "eslint": "^8.6.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "prettier": "^2.5.1"
  }
}
